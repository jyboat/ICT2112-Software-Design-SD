@model List<Dictionary<string, object>>

@{
    ViewData["Title"] = "View Patient Summaries";
}

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Patient ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var summary in Model){
            <tr>
                <td>@summary["patientId"]</td>
            </tr>
        }
    </tbody>
</table>
