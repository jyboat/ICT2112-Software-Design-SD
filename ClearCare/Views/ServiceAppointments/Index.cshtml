@model Dictionary<string, object>

@{
    ViewData["Title"] = "Appointment Details";
}

<!DOCTYPE html>
<html>
<head>
    <title>Appointment Details</title>
</head>
<body>
    <h2>Appointment Details</h2>
    <p><strong>Appointment ID:</strong> @Model["AppointmentId"]</p>
    <p><strong>Patient ID:</strong> @Model["PatientId"]</p>
    <p><strong>Nurse ID:</strong> @Model["NurseId"]</p>
    <p><strong>Doctor ID:</strong> @Model["DoctorId"]</p>
    <p><strong>Service Type:</strong> @Model["ServiceTypeId"]</p>
    <p><strong>Status:</strong> @Model["Status"]</p>
    <p><strong>Date & Time:</strong> @Model["DateTime"]</p>
    <p><strong>Location:</strong> @Model["Location"]</p>
</body>
</html>
